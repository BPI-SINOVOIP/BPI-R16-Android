
PBC_PATH=$1

if [ -z "$PBC_PATH" ]
then
   echo "usage: pbc-setpath <path>"
   echo "
    Valid <path> values are: 
        /telecom/pb 
        /telecom/ich 
        /telecom/och 
        /telecom/mch 
        /telecom/cch 
        /SIM1/telecom/pb 
        /SIM1/telecom/ich 
        /SIM1/telecom/och 
        /SIM1/telecom/mch 
        /SIM1/telecom/cch "
   exit 1
fi

PBAP_CLIENT_ACTION="set_path --es path $PBC_PATH"


source include.sh